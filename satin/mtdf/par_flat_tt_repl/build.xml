<project
    name="ibis.apps.satin.mtdf.par_flat_tt_repl"
    default="build"
    basedir=".">

    <property name="ibis"        location="../../../.."/>
    <property name="build"       location="build"/>

    <property name="satin-main-class" value="Mtdf"/>

    <import file="${ibis}/apps/satin/build-satin-app.xml"/>
    <import file="${ibis}/apps/rmi/build-rmi-app.xml"/>

    <target name="build"
	    depends="init"
	    description="build Mtdf application">
	<antcall inheritAll="true" inheritRefs="true" target="compile"/>
	<antcall inheritRefs="true" target="rmic-sun"/>
	<antcall inheritRefs="true" target="satin"/>
	<antcall inheritRefs="true" target="iogen"/>
    </target>

</project>


