<project
    name="ibis.apps.satin.barnes_inline"
    default="build"
    basedir=".">

    <property name="ibis"        location="../../.."/>
    <property name="build"       value="build"/>

    <property name="satinc-flags" value="-keep"/>
    <property name="main-class" value="BarnesHut"/>
    <property name="satin-main-class" value="${main-class}"/>

    <property name="test-par-run"
	      value="BarnesHut -tuple2 -v 100 5 -satin-closed"/>
    <property name="test-seq-run"
	      value="BarnesHut -tuple2 -v 100 5 -satin-closed"/>

    <fileset dir="." id="src.excludes-ref">
	<include name="BodyCanvas.java"/>
    </fileset>

    <import file="${ibis}/apps/satin/build-satin-app.xml"/>

</project>


