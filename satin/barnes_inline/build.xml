<project
    name="ibis.apps.satin.barnes_inline"
    default="build"
    basedir=".">

    <property name="ibis"        location="../../.."/>
    <property name="build"       location="build"/>

    <property name="satin-main-class" value="BarnesHut"/>

    <fileset dir="." id="src.excludes-ref">
	<include name="BodyCanvas.java"/>
    </fileset>

    <import file="${ibis}/apps/satin/build-satin-app.xml"/>

    <target name="test"
	    depends="build"
	    description="Run test">
	<antcall target="run2">
		<param name="runjob" value="BarnesHut -tuple2 -v 100 5 -satin-closed"/>
	</antcall>
    </target>
</project>


