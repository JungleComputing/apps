<project
    name="ibis.apps.satin.geneSequencing.dataDistribution"
    default="build"
    basedir=".">

    <property environment="env"/>

    <property name="ibis"        location="${env.IBIS_HOME}"/>
    <property name="build"       value="build"/>

    <path id="classpath">
	<fileset dir="../lib">
	    <include name="*.jar"/>
	</fileset>
	<fileset dir="../neobio/lib">
	    <include name="*.jar"/>
	</fileset>
    </path>

    <property name="user-classpath" refid="classpath"/>

    <property name="satinc-flags" value="-keep"/>

    <property name="main-class"  value="DsearchDC"/>

    <property name="satin-classes" value="DsearchDC,DivCon"/>

    <import file="${ibis}/build-files/apps/build-satin-app.xml"/>

</project>
