<project
    name="ibis.apps.satin.spawntest"
    default="build"
    basedir=".">

    <property name="ibis"        location="../../.."/>
    <property name="build"       location="build"/>

    <property name="satinc-flags" value=""/>

    <property name="satin-main-class" value="SpawnTest"/>

    <import file="${ibis}/apps/satin/build-satin-app.xml"/>

    <target name="test"
	    depends="build"
	    description="Run test">
	<antcall target="run2">
		<param name="runjob" value="SpawnTest -satin-closed"/>
	</antcall>
    </target>

</project>
