include ../../../../config.mk

java:
	cd arrays/read_in_place && $(MAKE) $(MFLAGS) java
	cd arrays/read_new && $(MAKE) $(MFLAGS) java
	cd list/final && $(MAKE) $(MFLAGS) java
	cd list/normal && $(MAKE) $(MFLAGS) java
	cd object/id && $(MAKE) $(MFLAGS) java
	cd tree/final && $(MAKE) $(MFLAGS) java
	cd tree/normal && $(MAKE) $(MFLAGS) java

manta:
	cd arrays/read_in_place && $(MAKE) $(MFLAGS) manta
	cd arrays/read_new && $(MAKE) $(MFLAGS) manta
	cd list/final && $(MAKE) $(MFLAGS) manta
	cd list/normal && $(MAKE) $(MFLAGS) manta
	cd object/id && $(MAKE) $(MFLAGS) manta
	cd tree/final && $(MAKE) $(MFLAGS) manta
	cd tree/normal && $(MAKE) $(MFLAGS) manta

clean:
	rm -f $(TRASH_FILES)
	cd arrays/read_in_place && $(MAKE) $(MFLAGS) clean
	cd arrays/read_new && $(MAKE) $(MFLAGS) clean
	cd list/final && $(MAKE) $(MFLAGS) clean
	cd list/normal && $(MAKE) $(MFLAGS) clean
	cd object/id && $(MAKE) $(MFLAGS) clean
	cd tree/final && $(MAKE) $(MFLAGS) clean
	cd tree/normal && $(MAKE) $(MFLAGS) clean
