	cd list-normal-conv; $(MAKE) $(MFLAGS) manta
	cd tree-final; $(MAKE) $(MFLAGS) manta
	cd tree-final-conv; $(MAKE) $(MFLAGS) manta
	cd tree-final-sun; $(MAKE) $(MFLAGS) manta
	cd object-array; $(MAKE) $(MFLAGS) manta	
	cd object-array-conv; $(MAKE) $(MFLAGS) manta
	cd int-array; $(MAKE) $(MFLAGS) manta	
	cd int-array-conv; $(MAKE) $(MFLAGS) manta
