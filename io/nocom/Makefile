clean:
	rm -f $(TRASH_FILES)
	cd array; $(MAKE) $(MFLAGS) clean	
	cd array-sun; $(MAKE) $(MFLAGS) clean	
	cd array-writeArray; $(MAKE) $(MFLAGS) clean	
	cd array-conv; $(MAKE) $(MFLAGS) clean
	cd array-conv-writeArray; $(MAKE) $(MFLAGS) clean
	cd list-final; $(MAKE) $(MFLAGS) clean
	cd list-final-conv; $(MAKE) $(MFLAGS) clean
	cd list-normal; $(MAKE) $(MFLAGS) clean
	cd list-normal-conv; $(MAKE) $(MFLAGS) clean
	cd tree-final; $(MAKE) $(MFLAGS) clean
	cd tree-final-conv; $(MAKE) $(MFLAGS) clean
	cd tree-final-sun; $(MAKE) $(MFLAGS) clean
	cd object-array; $(MAKE) $(MFLAGS) clean	
	cd object-array-conv; $(MAKE) $(MFLAGS) clean
	cd int-array; $(MAKE) $(MFLAGS) clean	
	cd int-array-conv; $(MAKE) $(MFLAGS) clean
	cd dlist-final; $(MAKE) $(MFLAGS) clean
