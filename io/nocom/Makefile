include ../../../config.mk

java: 
	cd array; $(MAKE) $(MFLAGS) java
	cd array-sun; $(MAKE) $(MFLAGS) java	
	cd array-writeArray; $(MAKE) $(MFLAGS) java	
	cd array-conv; $(MAKE) $(MFLAGS) java
	cd array-conv-writeArray; $(MAKE) $(MFLAGS) java
	cd list-final; $(MAKE) $(MFLAGS) java
	cd list-final-conv; $(MAKE) $(MFLAGS) java
	cd list-normal; $(MAKE) $(MFLAGS) java
	cd list-normal-conv; $(MAKE) $(MFLAGS) java
	cd tree-final; $(MAKE) $(MFLAGS) java
	cd tree-final-conv; $(MAKE) $(MFLAGS) java
	cd tree-final-sun; $(MAKE) $(MFLAGS) java
	cd object-array; $(MAKE) $(MFLAGS) java	
	cd object-array-conv; $(MAKE) $(MFLAGS) java
	cd int-array; $(MAKE) $(MFLAGS) java	
	cd int-array-conv; $(MAKE) $(MFLAGS) java
	cd dlist-final; $(MAKE) $(MFLAGS) java
	cd dlist-final-conv; $(MAKE) $(MFLAGS) java

