<project
    name="ibis.apps.rmi.barnes"
    default="jar"
    basedir=".">

    <description>
	Ibis RMI application build.
    </description>

    <property name="main-class"  value="BarnesHut"/>

    <property environment="env"/>

    <property name="ibis"        location="${env.IBIS_HOME}"/>

    <property name="shared-dir"	 location="../shared"/>

    <property name="test-seq-run" value="BarnesHut -N50000 -M10 -theta 2.0 -tstop 0.10 -dtime 0.025 -eps 0.00000025 -das -threads"/>
    <property name="test-par-run" value="BarnesHut -N50000 -M10 -theta 2.0 -tstop 0.10 -dtime 0.025 -eps 0.00000025 -das -threads"/>

    <import file="${ibis}/build-files/apps/build-rmi-app.xml"/>

</project>
